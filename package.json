  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Steph0/dotenv-configserver/issues"
  },
  "homepage": "https://github.com/Steph0/dotenv-configserver",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/github": "^5.0.0",
    "@actions/io": "^1.1.0",
    "@actions/tool-cache": "^1.7.1",
    "dotenv": "^10.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "@vercel/ncc": "^0.31.1",
    "jest": "^28.0.2"
