  "bugs": {
    "url": "https://github.com/Steph0/dotenv-configserver/issues"
  },
  "homepage": "https://github.com/Steph0/dotenv-configserver",
  "dependencies": {
    "@actions/core": "^1.3.0",
    "@actions/github": "^5.0.0",
    "@actions/io": "^1.1.0",
    "@actions/tool-cache": "^2.0.1",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.27.0",
    "jest": "^29.4.1"
